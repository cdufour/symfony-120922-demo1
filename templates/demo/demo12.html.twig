{% extends "base.html.twig" %}

{% block title %}Ciao !{% endblock %}

{% block body %}

<h1>{{ title }}</h1>
<h2 class="demo">Etudiants</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Job</th>
        </tr>
    </thead>
    <tbody>
        {% for student in students %}
        <tr>
            <td>{{ student.name }}</td>
            <td>{{ student.job }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>

{% endblock %}